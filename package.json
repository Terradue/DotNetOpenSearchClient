{
  "dependencies": {
    "auto-changelog": "^2.4.0"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": false,
    "commitLimit": false,
    "hideEmptyReleases": true,
    "ignoreCommitPattern": "^(.{0,5}|.*version bump.*|bump.*|ready.*|.*changelog.*|.*formatting.*|.*typo.*|.*Merge\\ tag.*)$",
    "releaseSummary": true
  }
}
